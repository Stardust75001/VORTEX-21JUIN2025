{%- style -%}

  /* Bordure */
  {% if section.settings.divider_option == 'space' %}
    .divider_wide {
      padding-top:{{ section.settings.space_two_element }}px;
    }
  {% elsif section.settings.divider_option == 'wave_custom' %}
    #wave_custom-{{ section.id }} {
      padding: 0;
      left: calc(-0%/2);
      width: calc(100% + 0%);
      height: var(--wave-fixe-height-desktop);
    }
    @media (max-width: 768px) {
         #wave_custom-{{ section.id }} {
             height: var(--wave-fixe-height-mobile);
             min-height: 20px;
        }
    }
  {% elsif section.settings.divider_option == 'wave_animated' %}
    /* Waves Animated */
     .sp-waves-section {
         position: relative;
         height: 0px;
         background-color: transparent;
    }
     .sp-waves {
         position: absolute;
         width: 100%;
         height: var(--wave-height-desktop);
         min-height: 20px;
         max-height: 150px;
         z-index: 1019;
    }
     .sp-waves-top {
         bottom: 0;
         left: 0;
    }
     .sp-waves-bottom {
         top: 0;
         left: 0;
         transform: rotate(180deg);
    }
    .sp-waves-parallax2 {
         animation: sp-waves-animation 25s cubic-bezier(.55, .5, .45, .5) infinite;
         animation-duration: calc(var(--wave-animation-speed) * 0.5);
    }
     .sp-waves-parallax>use {
         animation: sp-waves-animation 25s cubic-bezier(.55, .5, .45, .5) infinite;
    }
     .sp-waves-parallax > use:nth-child(1) {
         animation-delay: -2s;
         animation-duration: calc(var(--wave-animation-speed) * 0.5);
    }
     .sp-waves-parallax > use:nth-child(2) {
         animation-delay: -3s;
         animation-duration: calc(var(--wave-animation-speed) * 1);
    }
     .sp-waves-parallax > use:nth-child(3) {
         animation-delay: -4s;
         animation-duration: calc(var(--wave-animation-speed) * 1.5);
    }
     .sp-waves-parallax > use:nth-child(4) {
         animation-delay: -5s;
         animation-duration: calc(var(--wave-animation-speed) * 2);
    }
     @keyframes sp-waves-animation {
         0% {
             transform: translate3d(-90px, 0, 0);
        }
         100% {
             transform: translate3d(85px, 0, 0);
        }
    }
     @media (max-width: 768px) {
         .sp-waves {
             height: var(--wave-height-mobile);
             min-height: 20px;
        }
    }
    
  {% elsif section.settings.divider_option == 'border_custom' %}
     #border_custom {
       padding: 0;
       left: calc(-0%/2);
       width: calc(100% + 0%);
    }  
  {% endif %}

    #custom-border-{{ section.id }} {
    overflow: hidden;
    }
  
    #shopify-section-{{ section.id }} .border_svg_top svg {
      margin-bottom: -10px;
    }
    #shopify-section-{{ section.id }} .border_svg_bottom svg {
      margin-bottom: -10px;
    }
  
  .section-{{ section.id }}.wave_custom1,
  .section-{{ section.id }}.wave_custom2,
  .section-{{ section.id }}.wave_custom3,
  .section-{{ section.id }}.wave_custom4,
  .section-{{ section.id }}.wave_custom5,
  .section-{{ section.id }}.wave_custom6,
  .section-{{ section.id }}.wave_custom7,
  .section-{{ section.id }}.wave_custom8,
  .section-{{ section.id }}.wave_custom9,
  .section-{{ section.id }}.wave_custom10,
  .section-{{ section.id }}.wave_custom11,
  .section-{{ section.id }}.wave_custom12,
  .section-{{ section.id }}.wave_custom13 {
    background-color: {{ section.settings.color_border_fond_svg }};
    fill: {{ section.settings.color_border_svg }};
}
  
{%- endstyle -%}

