                    <radialGradient id="pulsing-gradient" cx="50%" cy="50%" r="50%" fx="50%" fy="50%">
                      <stop offset="0%" style="stop-color: {{ section.settings.color_border_svg_animated }}; stop-opacity: 1">
                        <animate attributeName="stop-opacity" values="1; 0; 1" dur="{{ section.settings.animation_speed }}s" repeatCount="indefinite" />
                      </stop>
                      <stop offset="100%" style="stop-color: {{ section.settings.color_border_fond_svg_animated }}; stop-opacity: 0">
                        <animate attributeName="stop-opacity" values="0; 1; 0" dur="{{ section.settings.animation_speed }}s" repeatCount="indefinite" />
                      </stop>
